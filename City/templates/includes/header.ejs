    <nav class="mobile main-nav hide-on-desktop" role="navigation">
        
        <ul id="menu-header" class="menu">
            <% menus.forEach(function (menu) { %>
		<li class="nav-features"><a href="<%= menu.link %>"><%= menu.name %></a></li>
		<% }); %>
</ul>

        <div class="social">
                    <% ['rss', 'twitter', 'github', 'facebook','linkedin','telegram','instagram','qq','weibo','youtube'].forEach((item) => { %>
                      <% if (site.customConfig[item]) { %>	
                <a href="<%= site.customConfig[item] %>" class="twitter"><i class="fa fa-<%= item %>"></i></a>
                  <% } %>
                           <% }) %>
        </div>
        
    </nav>

    <!-- end: .mobile.main-nav -->
    
    <div class="menu-overlay hide-on-desktop"></div>

