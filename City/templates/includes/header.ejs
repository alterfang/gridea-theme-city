            <div class="logo"><a href="<%= themeConfig.domain %>"><img src="<%= themeConfig.domain %>/images/avatar.png?v=<%= site.utils.now %>" alt="<%= themeConfig.siteName %>"></a></div>
        
        <!-- start: .main-nav -->
        <nav class="main-nav hide-on-tablet hide-on-mobile" role="navigation">
            
            <ul id="menu-header" class="menu">
